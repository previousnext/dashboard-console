{
  "name": "previousnext/dashboard-console",
  "description": "A console to display dashboard information from the PNX Dashboard API.",
  "require": {
    "symfony/console": "~2.7",
    "guzzlehttp/guzzle": "~6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "drupal/coder": "~8.0",
    "clue/phar-composer": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "PNX\\Dashboard\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {"PNX\\Dashboard\\Tests\\": ["tests/"]}
  },
  "license": "proprietary",
  "authors": [
    {
      "name": "Kim Pepper",
      "email": "kim@previousnext.com.au"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/"
  },
  "bin": ["dashboard.php"]
}
